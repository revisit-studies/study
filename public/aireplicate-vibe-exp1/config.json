{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/v2.2.0/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Quantifying Visualization Vibes (Study 1)",
    "version": "1.0",
    "authors": [],
    "date": "2024-12-19",
    "description": "This study investigated whether social attributions about a visualization's creator and context could be elicited asynchronously via an online survey. Participants viewed a series of five message-obscured visualizations and answered questions about the perceived skills, politics, values, and identity of the creator.",
    "organizations": []
  },
  "uiConfig": {
    "contactEmail": "contact@revisit.dev",
    "helpTextPath": "aireplicate-vibe-exp1/assets/help.md",
    "logoPath": "revisitAssets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": true,
    "nextOnEnter": true
  },
  "baseComponents": {
    "trial": {
      "type": "image",
      "style": {
        "width": "800px"
      },
      "nextButtonLocation": "sidebar",
      "instruction": "Please look at this visualization and answer the following questions about your perceptions of the creator.",
      "response": [
        {
          "id": "graphic-design-expertise",
          "prompt": "Are the person(s) most likely responsible for creating this image... likely ___ or ___ in graphic design?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "layperson",
              "value": 0
            },
            {
              "label": "professional",
              "value": 100
            }
          ]
        },
        {
          "id": "data-analysis-expertise",
          "prompt": "Are they likely ___ or ___ in data analysis?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "layperson",
              "value": 0
            },
            {
              "label": "professional",
              "value": 100
            }
          ]
        },
        {
          "id": "political-leaning",
          "prompt": "Are their politics more likely ___ or ___?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "left-leaning",
              "value": 0
            },
            {
              "label": "right-leaning",
              "value": 100
            }
          ]
        },
        {
          "id": "value-alignment",
          "prompt": "Do they likely share your values?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "does NOT share",
              "value": 0
            },
            {
              "label": "DOES share",
              "value": 100
            }
          ]
        },
        {
          "id": "intent",
          "prompt": "Is their intent more likely to ___ or ___?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "inform",
              "value": 0
            },
            {
              "label": "persuade",
              "value": 100
            }
          ]
        },
        {
          "id": "trustworthiness",
          "prompt": "How trustworthy are they?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "untrustworthy",
              "value": 0
            },
            {
              "label": "trustworthy",
              "value": 100
            }
          ]
        },
        {
          "id": "aesthetic-appeal",
          "prompt": "How aesthetically pleasing is the image?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "not at all",
              "value": 0
            },
            {
              "label": "very much",
              "value": 100
            }
          ]
        },
        {
          "id": "maker-identity",
          "prompt": "Who do you think is most likely responsible for having this image created?",
          "location": "sidebar",
          "type": "radio",
          "options": [
            "Individual",
            "organization",
            "education",
            "business",
            "political"
          ]
        },
        {
          "id": "confidence-maker-identity",
          "prompt": "Following your response to 'Maker Identity', how confident are you in your answer?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "0",
              "value": 0
            },
            {
              "label": "100",
              "value": 100
            }
          ]
        },
        {
          "id": "generation",
          "prompt": "What generation are they most likely from?",
          "location": "sidebar",
          "type": "radio",
          "options": [
            "boomer",
            "millennial",
            "gen-z"
          ]
        },
        {
          "id": "confidence-age",
          "prompt": "Following your response to 'Maker Age', how confident are you in your answer?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "0",
              "value": 0
            },
            {
              "label": "100",
              "value": 100
            }
          ]
        },
        {
          "id": "gender",
          "prompt": "What gender do they most likely identify with?",
          "location": "sidebar",
          "type": "radio",
          "options": [
            "other",
            "female",
            "male"
          ]
        },
        {
          "id": "confidence-gender",
          "prompt": "Following your response to 'Maker Gender', how confident are you in your answer?",
          "location": "sidebar",
          "type": "slider",
          "startingValue": 50,
          "options": [
            {
              "label": "0",
              "value": 0
            },
            {
              "label": "100",
              "value": 100
            }
          ]
        },
        {
          "id": "feed-behavior",
          "prompt": "As you're scrolling through your feed, you see this image. What would you do?",
          "location": "sidebar",
          "type": "radio",
          "options": [
            "scroll past",
            "stop and read"
          ]
        },
        {
          "id": "reasoning",
          "prompt": "Please explain your reasoning for the answers above.",
          "location": "sidebar",
          "type": "longText",
          "placeholder": "Enter your reasoning here..."
        }
      ]
    }
  },
