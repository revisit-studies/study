{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/v2.3.2/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Interactive Selections in Scatterplots",
    "version": "pilot",
    "authors": [
      "The reVISit Team"
    ],
    "date": "2023-11-03",
