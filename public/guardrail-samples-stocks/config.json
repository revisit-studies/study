{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/dev/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Evaluating Line Charts with Stock Data",
    "version": "pilot",
    "authors": ["VDL"],
    "date": "2024-01-11",
    "description": "A study challenging participants to review linecharts with a focus on stock market data.",
    "organizations": ["University of Utah"]
  },
  "uiConfig": {
    "contactEmail": "abrar.nadib@utah.edu",
    "logoPath": "assets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": false,
    "urlParticipantIdParam": "PROLIFIC_PID",
    "studyEndMsg": "**Thank you for completing the study. You may click this link and return to Prolific**: [https://app.prolific.com/submissions/complete?cc=C18Y4XD3](https://app.prolific.com/submissions/complete?cc=C18Y4XD3)",
    "enumerateQuestions": true,
    "stylesheetPath": "stage-1/assets/form.css"
  },
  "baseComponents": {
    "stock": {
      "instruction": "On Jan 1, 2024, a financial advisor told you that the stock highlighted below is one of the best investments you can make. The visualization depicts how it panned out. Please review the chart and answer the questions below.",
      "instructionLocation": "aboveStimulus",
      "type": "react-component",
      "path": "viz-guardrails/DataExplorer.tsx",
      "parameters": {
        "dataset": "sp500_stocks",
        "start_date": "2024-01-01",
        "end_date": "2024-12-24",
        "allow_time_slider": false,
        "allow_guardrail_selector": false,
        "allow_selection": false,
        "allow_help": false,
        "guardrail": "super_summ",
        "caption": "",
        "x_var": "date",
        "y_var": "value",
        "cat_var": "name",
        "group_var": "region"
      },
      "nextButtonLocation": "belowStimulus",
      "response": [
        {
          "id": "trust",
          "type": "likert",
          "location": "belowStimulus",
          "prompt": "How trustworthy is this chart?",
          "numItems": 7,
          "rightLabel": "Completely trustworthy",
          "leftLabel": "Not at all trustworthy",
          "required": true
        },
        {
          "id": "accuracy",
          "type": "slider",
          "location": "belowStimulus",
          "prompt": "How well did this stock perform compared to all other stocks in the market? Drag the slider to answer (1 means 'worst' and 100 means 'best ')",
          "options": [
            {
              "value": 1,
              "label": "1"
            },
            {
              "value": 25,
              "label": "25"
            },
            {
              "value": 50,
              "label": "50"
            },
            {
              "value": 75,
              "label": "75"
            },
            {
              "value": 100,
              "label": "100"
            }
          ],
          "required": true,
          "step": 1
        },
        {
          "id": "completeness",
          "type": "likert",
          "location": "belowStimulus",
          "prompt": "Do you feel that the visualization provided appropriate context to make an informed decision about your investment?",
          "numItems": 7,
          "leftLabel": "Not at all",
          "rightLabel": "Definitely",
          "required": true
        }
      ]
    },
    "stock-guardrail-selection-task": {
      "type": "react-component",
      "path": "viz-guardrails/GuardrailPreferenceTask.tsx",
      "instruction": "Below are four different visualizations (Aâ€“D) of stock data for Verizon (VZ). Please review each chart and select the one you think is the most useful comparison for Verizon.",
      "parameters": {
        "dataset": "sp500_stocks",
        "start_date": "2024-01-01",
        "end_date": "2024-12-24",
        "x_var": "date",
        "y_var": "value",
        "cat_var": "name",
        "group_var": "sector",
        "selection": ["VZ"],
        "range": null,
        "items": [],
        "data": [],
        "metadataFiltered": false,
        "numRandomSamples": 5,
        "numQuantiles": 5
      },
      "response": [
        {
          "id": "explanation",
          "prompt": "Please briefly explain why you chose that chart. Which aspects influenced your choice?",
          "secondaryText": "",
          "location": "belowStimulus",
          "type": "longText",
          "placeholder": "Enter your reasoning here"
        }
      ]
    }
  },
